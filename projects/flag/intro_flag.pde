// Choose some settings for the project
size(600, 400);
noStroke();

// Make the background white
background(255, 255, 255);

// Draw a red circle
fill(200, 0, 0);
ellipse(300, 200, 300, 300);

// Draw a blue rectangle
fill(0, 0, 200);
rect(0, 0, 200, 200);